\newcommand{\beamer@theme@ciad@authors@format@long}[1]{\underline{#1}}
\newcommand{\beamer@theme@ciad@authors@format@short}[1]{\textbf{#1}}
\newcommand{\beamer@theme@ciad@authors@longprefix}[1]{#1}
